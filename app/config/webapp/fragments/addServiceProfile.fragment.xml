<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout">
    <Dialog title="Create Field" class="sapUiSmallMargin">
        <VBox class="sapUiContentPadding sapUiSmallMargin">
            <Label  text="Profile Name :" />
            <Input  value="{addServiceProfileModel>/profileName}" placeholder="Enter field name" />
            <Label text="Profile Description :" />
            <Input value="{addServiceProfileModel>/profileDesc}" placeholder="Enter field name description" />
            
        </VBox>
        <footer>
        	<OverflowToolbar >	
                <ToolbarSpacer/>		
				<Button type="Accept" text="Save" press="onsaveNewProfile">
				</Button>
                <Button type="Reject" text="Cancel" press="oncancelNewProfile">
				</Button>
			</OverflowToolbar>
            <!-- <Button id="_IDGenButton3" text="Create" press="onCreateField" />
            <Button id="_IDGenButton4" text="Cancel" press="onCancelCreateField" /> -->
        </footer>
    </Dialog>
</core:FragmentDefinition>   